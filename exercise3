class Vehicle:
    def __init__(self, name, max_speed, mileage):
        self.name = name
        self.max_speed = max_speed
        self.mileage = mileage

    def print_abc(self):
        print(f"Vehicle Name:{self.name} Speed:{self.max_speed} Mileage:{self.mileage}")


class Bus(Vehicle):
    pass


carX = Bus("School Volve", 180, 12)

print("Vehicle Name:", carX.name, "Speed:", carX.max_speed, "Mileage:", carX.mileage)
